{
  "name": "react-kickstart",
  "version": "1.0.0",
  "description": "just another react + webpack boilerplate.",
  "private": true,
  "author": "Alessandro Arnodo <alessandro@arnodo.net> (https://alessandro.arnodo.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vesparny/react-kickstart/issues"
  },
  "homepage": "https://github.com/vesparny/react-kickstart",
  "main": "index.js",
  "scripts": {
    "test": "`npm bin`/mocha --require ./test/test-init --compilers js:babel/register --recursive",
    "test:cov": "`npm bin`/istanbul cover `npm bin`/_mocha -- --require ./test/test-init --compilers js:babel/register --recursive",
    "start": "`npm bin`/nodemon index.js",
    "build": "rm -rf dist && NODE_ENV=production `npm bin`/webpack --config ./webpack.config.js --progress --profile --colors",
    "gh-pages": "./scripts/publish-gh-pages",
    "eslint": "`npm bin`/eslint .",
    "jscs": "`npm bin`/jscs ."
  },
  "dependencies": {
    "basscss": "^6.1.6",
    "fastclick": "^1.0.6",
    "react": "^0.13.3",
    "react-router": "^0.13.3"
  },
  "devDependencies": {
    "babel": "^5.6.1",
    "babel-core": "^5.6.1",
    "babel-eslint": "^3.1.17",
    "babel-jscs": "^1.0.2",
    "babel-loader": "^5.1.4",
    "chai": "^3.0.0",
    "css-loader": "^0.15.1",
    "eslint": "^0.23.0",
    "eslint-plugin-react": "^2.5.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "istanbul": "^0.3.15",
    "jscs": "^1.13.1",
    "jsdom": "3.1.2",
    "json-loader": "^0.5.2",
    "mocha": "^2.2.5",
    "mocha-jsdom": "^0.4.0",
    "nodemon": "^1.3.7",
    "react-hot-loader": "^1.2.7",
    "stats-webpack-plugin": "^0.1.2",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  }
}
